// 0
λ λ _0;;

// 1
λ λ (_1 _0);;

// add
λ λ λ λ _3 _1 (_2 _1 _0);;

// mult
λ λ λ λ _3 (_2 _1) _0;;

// power
λ λ λ λ (_2 _3) _1 _0;;

// pred = λ n f x. n (λ g h. h (g f)) (λ u. x) (λ u. u)}
λ λ λ _2 (λ λ _0 (_1 _3)) (λ _1) (λ _0);;

// -------- List --------
// []
λ λ _0;;

// [.]
λ λ λ _1 _2 _0;;

// cons
λ λ λ λ _1 _3 (_2 _1 _0);;

// append
λ λ λ λ (_3 _1 (_2 _1 _0));;

//hd
λ _0 (λ λ _1) 0;;







